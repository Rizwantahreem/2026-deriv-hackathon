  "validation_rules": {
    "cnic": {
      "description": "Pakistan CNIC",
      "regex": "^\\d{5}-\\d{7}-\\d{1}$",
      "format": "#####-#######-#",
      "example": "12345-1234567-1"
    },
    "aadhaar": {
      "description": "India Aadhaar",
      "regex": "^\\d{4}\\s?\\d{4}\\s?\\d{4}$",
      "format": "#### #### ####",
      "example": "1234 5678 9012"
    },
    "pan": {
      "description": "India PAN",
      "regex": "^[A-Z]{5}[0-9]{4}[A-Z]{1}$",
      "format": "AAAAA9999A",
      "example": "ABCDE1234F"
    },
    "ni_number": {
      "description": "UK National Insurance",
      "regex": "^[A-CEGHJ-PR-TW-Z]{2}\\s?\\d{2}\\s?\\d{2}\\s?\\d{2}\\s?[A-D]$",
      "format": "AA 99 99 99 A",
      "example": "QQ 12 34 56 A"
    },
    "uk_postcode": {
      "description": "UK Postcode",
      "regex": "^[A-Z]{1,2}\\d[A-Z\\d]?\\s?\\d[A-Z]{2}$",
      "format": "AA9A 9AA",
      "example": "SW1A 1AA"
    },
    "pk_postal": {
      "description": "Pakistan Postal Code",
      "regex": "^\\d{5}$",
      "format": "#####",
      "example": "44000"
    },
    "in_pin": {
      "description": "India PIN Code",
      "regex": "^\\d{6}$",
      "format": "######",
      "example": "400001"
    }
  },
  
